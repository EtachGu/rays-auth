        <pagehelper-spring-boot-starter.version>1.2.10</pagehelper-spring-boot-starter.version>
        <mybatis-generator.version>1.3.5</mybatis-generator.version>
        <mybatis-autoconfig.version>1.3.2</mybatis-autoconfig.version>
        <spring-boot-plugin.version>2.1.1.RELEASE</spring-boot-plugin.version>
        <mybatis-spring-boot-starter.version>1.3.1</mybatis-spring-boot-starter.version>
        <mapper-spring-boot-starter.version>2.1.1</mapper-spring-boot-starter.version>
        <alibaba-druid.version>1.1.10</alibaba-druid.version>
        <thymeleaf.version>2.1.1.RELEASE</thymeleaf.version>
        <thymeleaf.extras-springsecurity.version>3.0.4.RELEASE</thymeleaf.extras-springsecurity.version>
        <spring-boot-start-data-redis.version>2.1.3.RELEASE</spring-boot-start-data-redis.version>
        <spring-boot-starter-logging.version>2.1.3.RELEASE</spring-boot-starter-logging.version>

        <google.guava.version>23.0</google.guava.version>
        <apache-common-lang3.version>3.8.1</apache-common-lang3.version>
        <jjwt.version>0.9.1</jjwt.version>

        <webjars.bootstrap.version>4.3.1</webjars.bootstrap.version>
        <webjars.jquery.version>3.3.1-2</webjars.jquery.version>
        <webjars.locator.version>0.36</webjars.locator.version>
    </properties>

